{
    "scripts": {
        "dev": "turbo run dev --parallel",
        "start": "turbo run start --parallel",
        "build": "turbo run build --parallel",
        "lint": "turbo run lint --parallel && prettier . --check",
        "clean": "turbo run clean --parallel && rm -rf out node_modules target Cargo.lock",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.6.7",
        "@commitlint/config-conventional": "^17.6.7",
        "@types/eslint": "^8.44.2",
        "@types/node": "^20.4.8",
        "eslint": "^8.46.0",
        "husky": "^8.0.3",
        "prettier": "^3.0.1",
        "turbo": "^1.10.12",
        "typescript": "^5.1.6"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "packageManager": "pnpm@8.6.12",
    "engines": {
        "node": "18"
    }
}
